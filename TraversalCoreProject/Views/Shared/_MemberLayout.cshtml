                    <a href="/Member/LastDestinations/Index/"><i class="ft-layout"></i><span class="menu-title" data-i18n="">@localizer["son rotalar"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Default/Index/"><i class="ft-layout"></i><span class="menu-title" data-i18n="">@localizer["siteye git"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="#"><i class="ft-layout"></i><span class="menu-title" data-i18n="">@localizer["cikis yap"]</span></a>
                </li>
            </ul>

        </div>
        <div class="navigation-background"></div>
    </div>



    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            @await Component.InvokeAsync("_MemberLayoutHeaderContent")
            <div class="content-body">
                @RenderBody()
            </div>
        </div>
    </div>
